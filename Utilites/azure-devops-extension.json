            "properties": {
                "name": "Powershell++"
            }
        },
        {
            "id": "task-Shellpp",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Shell++"
            }
        },
        {
            "id": "task-zip",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Zip"
            }
        },
        {
            "id": "task-unzip",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "UnZip"
            }
        },
        {
            "id": "task-rollback",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Rollback"
            }
