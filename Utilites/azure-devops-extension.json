            "path": "images/screen1.png"
        },
        {
            "path": "images/screen1.png"
        },
        {
            "path": "images/screen1.png"
        }
    ],
    "categories": [
        "Azure pipelines"
    ],
    "tags": [
        "utility",
        "tasks"
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/microsoft/Extension-UtilitiesPack/blob/master/Utilites/overview.md"
        },
        "issues": {
            "uri": "https://github.com/microsoft/Extension-UtilitiesPack/issues"
        },
        "repository": {
            "uri": "https://github.com/microsoft/Extension-UtilitiesPack"
        }
    },
    "branding": {
        "color": "#f6f7fb",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "../LICENSE"
        }
    },
    "files": [
        {
            "path": "Tokenizer"
        },
        {
            "path": "Powershell++"
        },
        {
            "path": "Shell++"
        },
        {
            "path": "Zip"
        },
        {
            "path": "UnZip"
        },
        {
            "path": "Rollback"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "scopes": [
        "vso.build_execute",
        "vso.serviceendpoint_manage"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "task-tokenizer",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tokenizer"
            }
        },
        {
            "id": "task-PSpp",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
