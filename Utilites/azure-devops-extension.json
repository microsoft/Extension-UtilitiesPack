{
    "manifestVersion": 1,
    "id": "utilitytasks",
    "name": "Release Management Utility tasks",
    "version": "0.1.10",
    "publisher": "ms-devlabs",
    "description": "Utility tasks for Release Management",
    "public": true,
    "icons": {
        "default": "images/Icon128.png",
        "large": "images/Icon512.png"
